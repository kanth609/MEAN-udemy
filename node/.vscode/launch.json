{
  "version": "0.2.0",
  "configurations": [
    {
      "command": "npm run serve",
      "name": "Run npm serve",
      "request": "launch",
      "type": "node-terminal"
    },
    {
      "name": "Debug node process",
      "type": "node",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "program": "${workspaceFolder}/server.js",
      "skipFiles": ["<node_internals/**"],
      "stopOnEntry": true,
      "console": "externalTerminal"
    }
  ]
}
